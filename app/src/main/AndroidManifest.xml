<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shater">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.RequestActivity"></activity>
        <activity android:name=".activity.ViewAccountOfAdminActivity" />
        <activity android:name=".activity.AdminActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.HomeCustomerActivity" />
        <activity android:name=".activity.HomeServiceActivity" />
        <activity android:name=".activity.SignUpActivity" />
        <activity android:name=".activity.UserActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>